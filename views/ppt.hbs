<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <link
    href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,700,700i%7CMaitree:200,300,400,600,700&amp;subset=latin-ext"
    rel="stylesheet">

  <link rel="stylesheet" type='text/css' media='all' href="/webslides/css/webslides.css">

  <link rel="stylesheet" type="text/css" media="all" href="/webslides/css/svg-icons.css">
  <title>{{title}}</title>
  <style>
    .specs li span {
      font-size: initial;
    }

    hr.black {
      background: radial-gradient(ellipse at center, rgba(255, 255, 255, 0.8) 0, rgba(255, 255, 255, 0) 75%);
    }
  </style>
</head>

<body>
  <main role="main">
    <article id="webslides">
      {{#each slides}}
        {{> (lookup . 'template') . }}
      {{/each}}
    </article>
  </main>
  <script src="/webslides/js/webslides.js"></script>
  <script>
    window.ws = new WebSlides({loop: false});
  </script>
  <script defer src="/webslides/js/svg-icons.js"></script>
</body>

</html>